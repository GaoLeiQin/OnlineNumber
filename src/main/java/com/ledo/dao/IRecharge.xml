<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ledo.dao.IRecharge">
	<select id="queryRechargeInfo" resultType="RechargeInfo">
		select zoneId,date,userId,roleId,rmbNum,platId,platName,chargePlatSn,chargeGameSn from rechargeinfo_test3
	</select>

	<select id="queryRechargeInfoByPage" parameterType="Page" resultType="RechargeInfo">
		select zoneId,date,userId,roleId,rmbNum,platId,platName,chargePlatSn,chargeGameSn from rechargeinfo_test3
		<if test="startRow != null and pageSize">
			limit #{startRow},#{pageSize}
		</if>
	</select>

</mapper>