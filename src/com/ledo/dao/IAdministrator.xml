<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ledo.dao.IAdministrator">

	<!--插入服务器在线数据-->
	<insert id="insertServerInfo" parameterType="ServerHistoryInfo">
		insert into online_number_2018_10(date,officialNum,mixNum,gatNum,totalNum)
		values(#{date},#{officialNum},#{mixNum},#{gatNum},#{totalNum})
	</insert>

	<select id="queryServerHistoryInfoByLimit25" resultType="ServerHistoryInfo">
		select date,officialNum,mixNum,gatNum,totalNum from online_number_2018_10 order by id desc limit 0,25
	</select>

	<!--删除Linux服务器信息-->
	<delete id="deleteLinuxServerInfo">
		delete from all_serverinfo_test4
	</delete>
	<!--插入Linux服务器相关信息-->
	<insert id="insertLinuxServerInfo" parameterType="AllServerInfo">
		insert into all_serverinfo_test4(channel,zoneId,serverName,optOrId,ip,hostName,onlineNum,openTime,openDays)
		values(#{channel},#{zoneId},#{serverName},#{optOrId},#{ip},#{hostName},#{onlineNum},#{openTime},#{openDays});
	</insert>

	<!--删除充值数据-->
	<delete id="deleteRechargeInfo">
		delete from rechargeinfo_test3
	</delete>
	<!--插入充值数据-->
	<insert id="insertRechargeInfo" parameterType="RechargeInfo">
		insert into rechargeinfo_test3(zoneId,date,userId,roleId,rmbNum,platId,platName,chargePlatSn,chargeGameSn)
		values(#{zoneId},#{date},#{userId},#{roleId},#{rmbNum},#{platId},#{platName},#{chargePlatSn},#{chargeGameSn});
	</insert>

</mapper>